<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<contentHaul xmlns:a="http://www.appian.com/ae/types/2009">
    <rule>
        <name>DEVASSURE_ResultPage</name>
        <uuid>_a-0000ddcb-da42-8000-3457-01ef9001ef90_2932</uuid>
        <description>Contains link for result page</description>
        <parentUuid>_a-0001ddc2-1c7c-8000-9ba2-011c48011c48_11548</parentUuid>
        <visibility>
            <advertise>false</advertise>
            <hierarchy>true</hierarchy>
            <indexable>true</indexable>
            <quota>false</quota>
            <searchable>true</searchable>
            <system>false</system>
            <unlogged>false</unlogged>
        </visibility>
        <definition>=#"SYSTEM_SYSRULES_formLayout"(
  label: "DEV ASSURE Validation Result",
  firstColumnContents: {
#"SYSTEM_SYSRULES_linkField"(
  links:#"SYSTEM_SYSRULES_documentDownloadLink"(document:ri!generatedDocument,
  label:"Click here to download the CSV file"
  )
)
  },
  secondColumnContents: {
    /* Add components here for a two-column form */
  },
  buttons: #"SYSTEM_SYSRULES_ButtonLayout"(
    primaryButtons: {
      #"SYSTEM_SYSRULES_ButtonWidgetSubmit"(
        label: "Continue",
        style: "PRIMARY",
        saveInto: ri!cancel,
        value:false
      )
    },
    secondaryButtons: {
      #"SYSTEM_SYSRULES_ButtonWidgetSubmit"(
        label: "Close",
        style: "NORMAL",
        value: true,
        saveInto: ri!cancel,
        skipValidation: true
      )
    }
  ),
  validations: {}
)</definition>
        <namedTypedValue>
            <name>cancel</name>
            <type>
                <name>boolean</name>
                <namespace>http://www.w3.org/2001/XMLSchema</namespace>
            </type>
        </namedTypedValue>
        <namedTypedValue>
            <name>generatedDocument</name>
            <type>
                <name>CollaborationDocument</name>
                <namespace>http://www.appian.com/ae/types/2009</namespace>
            </type>
        </namedTypedValue>
        <metadataExpr></metadataExpr>
        <preferredEditor>interface</preferredEditor>
        <offlineEnabled>false</offlineEnabled>
    </rule>
    <roleMap public="true">
        <role inherit="true" allowForAll="false" name="readers">
            <users/>
            <groups/>
        </role>
        <role inherit="true" allowForAll="false" name="authors">
